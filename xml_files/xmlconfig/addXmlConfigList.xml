<?xml version="1.0" encoding="UTF-8"?>
<Function id="addXmlConfigList" name="添加xml配置信息" datasource="dataSource">
    <Parameters>
        <addparams>
            <LOCATION/>
            <APP_NAME default=""/>
            <APP_CODE/>
            <CRT_PSN default=""/>
            <UPD_PSN default=""/>
        </addparams>
    </Parameters>
    <Actions>
        <insert id="insert" isreturn="true" params="addparams" errorid="1">
            insert into sys_xml_config (id，objid,LOCATION,APP_NAME,APP_CODE,CRT_PSN,UPD_PSN) values (sys_id.nextval,sys_guid(),#{LOCATION},#{APP_NAME},#{APP_CODE},#{CRT_PSN},#{UPD_PSN})
        </insert>
    </Actions>
</Function>